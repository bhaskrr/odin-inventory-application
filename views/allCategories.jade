extends layout 

block content
    section(class="categories text-white p-2 h-100")
        if allCategories.length
            ul(class="list-unstyled bg-secondary ")
                each category in allCategories
                    li(class='fs-3 p-3 border border-secondary-subtle') 
                        a(class="nav-link" href=category.url) #{category.name}

